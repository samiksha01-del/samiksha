@charset "utf-8";

:root { --primary: #5F0F40; --secondary: #9A031E; --accent: #FB8B24; --light: #E36414; --background: #0F4C5C; }

body { font-family: "Segoe UI", Tahoma, Geneva, Verdana, sans-serif; background-color: rgb(248, 248, 248); color: rgb(51, 51, 51); }

.gallery-item { transition: transform 0.3s, box-shadow 0.3s; }

.gallery-item:hover { transform: translateY(-5px); box-shadow: rgba(0, 0, 0, 0.2) 0px 10px 20px; }

.artist-card { transition: 0.3s; }

.artist-card:hover { transform: scale(1.03); }

.blur-load { background-size: cover; background-position: center center; }

.navbar { backdrop-filter: blur(10px); background-color: rgba(255, 255, 255, 0.8); }

.hero-section { background: linear-gradient(135deg, rgba(95, 15, 64, 0.8) 0%, rgba(15, 76, 92, 0.8) 100%); }

.price-tag { position: absolute; top: 10px; right: 10px; background-color: var(--accent); color: white; padding: 5px 10px; border-radius: 4px; font-weight: bold; }

@keyframes fadeIn { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

.animate-fadeIn { animation: 1s ease-in-out 0s 1 normal none running fadeIn; }

.spinner { width: 40px; height: 40px; border-width: 4px; border-style: solid; border-top-color: rgba(0, 0, 0, 0.1); border-right-color: rgba(0, 0, 0, 0.1); border-bottom-color: rgba(0, 0, 0, 0.1); border-image: initial; border-left-color: var(--primary); border-radius: 50%; animation: 1s linear 0s infinite normal none running spin; }

@keyframes spin { 
  100% { transform: rotate(360deg); }
}
